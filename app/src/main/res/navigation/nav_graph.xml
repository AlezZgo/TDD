<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.tddapp.main.FirstFragment"
        android:label="TDD features"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_FirstFragment_to_markdownFragment"
            app:destination="@id/markdownFragment" />
    </fragment>
    <fragment
        android:id="@+id/markdownFragment"
        android:name="com.example.tddapp.markdown.MarkdownFragment"
        android:label="Markdown feature"
        tools:layout="@layout/fragment_markdown" />
</navigation>